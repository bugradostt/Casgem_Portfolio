@model List<Casgem_Portfolio.Models.Entities.TblWhoAmI>

@foreach (var i in Model)
{

    <!-- home page about section -->
    <section class="w3l-index3" id="about">
        <div class="midd-w3 py-5">
            <div class="container py-lg-5 py-md-3">
                <div class="row">
                    <div class="col-lg-4">
                        <div class="position-relative">
                            <img src="@i.WhoAmIImgUrl" alt="" class="radius-image img-fluid">
                        </div>
                    </div>
                    <div class="col-lg-8 mt-lg-0 mt-5">
                        <h5 class="title-small mb-2">@i.WhoAmITitle</h5>
                        <h3 class="title-big">@i.WhoAmIJobs</h3>
                        <p class="mt-4" style="text-align:justify">
                            @i.WhoAmIDescription
                        </p>
                       
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- //home page about section -->


}
