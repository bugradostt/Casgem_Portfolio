
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<h3>İletişim</h3>
<hr />
<br />

<div>

    <div class="mt-3">

        <label>Telefon</label>
        <input type="text" disabled class="form-control" value="@ViewBag.phone" />

    </div>

    <div class="mt-3">

        <label>Mail</label>
        <input type="text" disabled class="form-control" value="@ViewBag.mail" />

    </div>

    <div class="mt-3">

        <label>Adres</label>
        <input type="text" disabled class="form-control" value="@ViewBag.adress" />

    </div>

    <div class="mt-3">

        <label>Harita</label>
        <div class="map-iframe">
            <iframe src="@ViewBag.maps"
                    width="100%" height="300"
                    frameborder="0" style="border: 0px;" allowfullscreen=""></iframe>
        </div>

    </div>




    <br />
    <a class="btn btn-warning" href="/Admin/EditContact/">Güncelle</a>


</div>